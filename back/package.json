{
  "name": "mini-kafka-ui",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "Sara Morillon",
    "url": "https://saramorillon.com/"
  },
  "repository": {
    "url": "https://github.com/saramorillon/mini-kafka-ui"
  },
  "scripts": {
    "build": "tsc",
    "start:dev": "nodemon",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "package:win32": "yarn build && electron-packager dist --platform=win32 --arch=x64 --overwrite --icon=public/favicon.ico",
    "package:linux": "yarn build && electron-packager dist --platform=linux --arch=x64 --overwrite --icon=public/favicon.ico"
  },
  "devDependencies": {
    "@saramorillon/eslint-config-typescript": "^1.1.0",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "electron": "^18.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-packager": "^15.5.1",
    "eslint": "^8.15.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "kafkajs": "^2.0.0"
  }
}
